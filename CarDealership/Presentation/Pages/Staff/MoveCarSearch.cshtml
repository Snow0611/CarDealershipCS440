@{
    ViewData["Title"] = "Car Search";
    Layout = null;
}

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using CarDealership.DataAccess

@model IEnumerable<Car>

<h2>Search for a car by plate number</h2>

<form action="/BusinessLogicLayer/MoveCarForm" method="get">
    <input type="text" name="plateNum" />
    <input type="submit" value="Submit">
</form> 